{
	"name": "svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently -n registry,app -c \"blue,green\" \"pnpm registry:dev\" \"vite dev\"",
		"build": "pnpm build:registry && vite build",
		"preview": "pnpm build:registry && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"registry:build": "jsrepo build",
		"registry:dev": "jsrepo build --watch"
	},
	"devDependencies": {
		"concurrently": "catalog:",
		"@sveltejs/adapter-auto": "^7.0.0",
		"@sveltejs/kit": "^2.47.1",
		"@sveltejs/vite-plugin-svelte": "^6.2.1",
		"@tailwindcss/vite": "^4.1.14",
		"clsx": "^2.1.1",
		"jsrepo": "workspace:*",
		"svelte": "^5.41.0",
		"svelte-check": "^4.3.3",
		"tailwind-merge": "^3.3.1",
		"tailwind-variants": "^3.1.1",
		"tailwindcss": "^4.1.14",
		"typescript": "^5.9.3",
		"vite": "^7.1.10"
	}
}
