{
	"name": "@example/svelte",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "concurrently -n registry,app -c \"blue,green\" \"pnpm registry:dev\" \"vite dev\"",
		"build": "pnpm build:registry && vite build",
		"preview": "pnpm build:registry && vite preview",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"registry:build": "jsrepo build",
		"registry:dev": "jsrepo build --watch"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^7.0.1",
		"@sveltejs/kit": "^2.53.0",
		"@sveltejs/vite-plugin-svelte": "^6.2.4",
		"@tailwindcss/vite": "^4.2.0",
		"clsx": "^2.1.1",
		"concurrently": "catalog:",
		"jsrepo": "workspace:*",
		"svelte": "^5.53.2",
		"svelte-check": "^4.4.3",
		"tailwind-merge": "^3.5.0",
		"tailwind-variants": "^3.2.2",
		"tailwindcss": "^4.2.0",
		"typescript": "^5.9.3",
		"vite": "^7.3.1"
	}
}
