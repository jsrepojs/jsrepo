		"npm-packlist": "^10.0.3",
		"picocolors": "catalog:"
	},
	"devDependencies": {
		"@types/node": "catalog:",
		"@types/npm-packlist": "^7.0.3",
		"@types/npmcli__arborist": "^6.3.1",
		"tsdown": "catalog:",
		"typescript": "catalog:",
		"zod": "catalog:"
	},
	"version": null
}
